{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (protocol, status_class) (rate(content_cache_http_requests_total{job=\"content-cache\"}[$__rate_interval]))",
          "legendFormat": "{{protocol}} {{status_class}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "Request Duration (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le) (rate(content_cache_http_request_duration_seconds_bucket{job=\"content-cache\"}[$__rate_interval])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(content_cache_http_request_duration_seconds_bucket{job=\"content-cache\"}[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(content_cache_http_request_duration_seconds_bucket{job=\"content-cache\"}[$__rate_interval])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "Cache Hit Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum(rate(content_cache_http_requests_total{job=\"content-cache\", cache_result=\"hit\"}[$__rate_interval])) / sum(rate(content_cache_http_requests_total{job=\"content-cache\", cache_result=~\"hit|miss\"}[$__rate_interval]))",
          "legendFormat": "hit rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "custom": { "drawStyle": "line", "fillOpacity": 20, "showPoints": "never" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.8 }
            ]
          },
          "color": { "mode": "continuous-GrYlRd" }
        },
        "overrides": []
      }
    },
    {
      "title": "Cache Hit / Miss / Bypass",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (cache_result) (rate(content_cache_http_requests_total{job=\"content-cache\", cache_result=~\"hit|miss|bypass\"}[$__rate_interval]))",
          "legendFormat": "{{cache_result}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "drawStyle": "bars", "fillOpacity": 50, "stacking": { "mode": "normal" }, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "Response Bytes Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (protocol) (rate(content_cache_http_response_bytes_total{job=\"content-cache\"}[$__rate_interval]))",
          "legendFormat": "{{protocol}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "Request Duration by Protocol (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, protocol) (rate(content_cache_http_request_duration_seconds_bucket{job=\"content-cache\"}[$__rate_interval])))",
          "legendFormat": "{{protocol}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "Requests by Protocol",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (protocol) (increase(content_cache_http_requests_total{job=\"content-cache\"}[$__range]))",
          "legendFormat": "{{protocol}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Requests by Status Class",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (status_class) (increase(content_cache_http_requests_total{job=\"content-cache\"}[$__range]))",
          "legendFormat": "{{status_class}}",
          "refId": "A"
        }
      ]
    },
    {
      "title": "Error Rate (4xx + 5xx)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum(rate(content_cache_http_requests_total{job=\"content-cache\", status_class=~\"4xx|5xx\"}[$__rate_interval]))",
          "legendFormat": "errors/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Requests by Endpoint",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "sum by (protocol, endpoint, cache_result) (increase(content_cache_http_requests_by_endpoint_total{job=\"content-cache\"}[$__range]))",
          "legendFormat": "",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true },
            "renameByName": {
              "protocol": "Protocol",
              "endpoint": "Endpoint",
              "cache_result": "Cache Result",
              "Value": "Requests"
            }
          }
        },
        {
          "id": "sortBy",
          "options": { "sort": [{ "field": "Requests", "desc": true }] }
        }
      ]
    },
    {
      "title": "GC",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 40 },
      "collapsed": false
    },
    {
      "title": "GC Runs",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 41 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "rate(content_cache_gc_runs_total{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "runs/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "GC Run Duration (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 41 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(content_cache_gc_run_duration_seconds_bucket{job=\"content-cache\"}[$__rate_interval]))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "GC Bytes Reclaimed",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 41 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "rate(content_cache_gc_bytes_reclaimed_total{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "reclaimed",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "GC Evictions & Deletions",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 49 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "rate(content_cache_gc_lru_blobs_evicted_total{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "LRU evicted",
          "refId": "A"
        },
        {
          "expr": "rate(content_cache_gc_orphan_blobs_deleted_total{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "orphan deleted",
          "refId": "B"
        },
        {
          "expr": "rate(content_cache_gc_unreferenced_blobs_deleted_total{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "unreferenced deleted",
          "refId": "C"
        },
        {
          "expr": "rate(content_cache_gc_expired_meta_deleted_total{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "expired meta deleted",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "GC Errors & Status",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 49 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "rate(content_cache_gc_errors_total{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "errors/s",
          "refId": "A"
        },
        {
          "expr": "content_cache_gc_last_run_success{job=\"content-cache\"}",
          "legendFormat": "last run success",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "Go Runtime",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 57 },
      "collapsed": false
    },
    {
      "title": "Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 58 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "go_memstats_heap_inuse_bytes{job=\"content-cache\"}",
          "legendFormat": "heap in-use",
          "refId": "A"
        },
        {
          "expr": "go_memstats_heap_alloc_bytes{job=\"content-cache\"}",
          "legendFormat": "heap alloc",
          "refId": "B"
        },
        {
          "expr": "go_memstats_stack_inuse_bytes{job=\"content-cache\"}",
          "legendFormat": "stack in-use",
          "refId": "C"
        },
        {
          "expr": "process_resident_memory_bytes{job=\"content-cache\"}",
          "legendFormat": "RSS",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "Goroutines",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 58 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "go_goroutines{job=\"content-cache\"}",
          "legendFormat": "goroutines",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 58 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "CPU",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": { "drawStyle": "line", "fillOpacity": 20, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "Open File Descriptors",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 66 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "process_open_fds{job=\"content-cache\"}",
          "legendFormat": "open",
          "refId": "A"
        },
        {
          "expr": "process_max_fds{job=\"content-cache\"}",
          "legendFormat": "max",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    },
    {
      "title": "GC Pause Duration",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 66 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "rate(go_gc_duration_seconds_sum{job=\"content-cache\"}[$__rate_interval])",
          "legendFormat": "GC time/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "drawStyle": "line", "fillOpacity": 10, "showPoints": "never" }
        },
        "overrides": []
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["content-cache"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "default", "value": "default" },
        "hide": 0,
        "includeAll": false,
        "label": "Data Source",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-15m", "to": "now" },
